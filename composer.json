    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "php-http/curl-client": "~1.7.0",
        "jakub-onderka/php-parallel-lint": "^0.9.2",
        "guzzlehttp/psr7": "^1.4",
        "ramsey/uuid": "^3.7",
