{
    "name": "klink/adapterboilerplate",
    "description": "The K-Link K-Search client",
    "type": "library",
    "homepage": "https://git.klink.asia/main/k-search-client-php",
    "time": "2017-04-24",

    "authors": [
        {
            "name": "Alessio Vertemati",
            "email": "alessio@oneofftech.xyz"
        },
        {
            "name": "Emanuele Panzeri",
            "email": "emanuele.panzeri@oneofftech.xyz"
        }
    ],

    "support": {
        "issues": "https://git.klink.asia/main/k-search-client-php/issues",
        "wiki": "https://git.klink.asia/main/k-search-client-php/wikis/home",
        "source": "https://git.klink.asia/main/k-search-client-php"
    },

    "autoload": {
        "psr-4": {
            "KSearchClient\\": "src/"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        
        "files": [
            "tests/helpers.php"
        ]
    },
    
    "require": {
        "php": ">=5.6.4",
        "jms/serializer": "^1.9",
        "symfony/yaml": "^3.2",
        "php-http/client-implementation": "^1.0",
        "php-http/message": "^1.6",
        "php-http/discovery": "^1.3",
        "doctrine/annotations": "^1.4",
        "doctrine/cache": "^1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "php-http/curl-client": "~1.7.0",
        "jakub-onderka/php-parallel-lint": "^0.9.2",
        "guzzlehttp/psr7": "^1.4",
        "ramsey/uuid": "^3.7"
    }
}
