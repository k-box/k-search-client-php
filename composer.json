{
    "name": "klink/adapterboilerplate",
    "description": "The Library for interacting with the K-Link Core",
    "type": "library",
    "homepage": "https://dev.klink.asia",
    "time": "2017-04-24",

    "authors": [
        {
            "name": "Alessio Vertemati",
            "email": "alessio@oneofftech.xyz"
        },
        {
            "name": "Emanuele Panzeri",
            "email": "emanuele.panzeri@oneofftech.xyz"
        }
    ],

    "support": {
        "issues": "https://git.klink.asia/kadapters/adapterboilerplate/issues",
        "wiki": "https://git.klink.asia/kadapters/adapterboilerplate/wikis/home",
        "source": "https://git.klink.asia/kadapters/adapterboilerplate"
    },

    "autoload": {
        "psr-4": {
            "KSearchClient\\": "src/",
            "JMS\\Serializer\\Annotation\\": "vendor/jms/serializer/src/JMS/Serializer/Annotation"
        }
    },

    "autoload-dev": {
        "psr-4": {"KSearchClient\\Tests\\": "tests/"}
    },
    
    "require": {
        "php": ">=5.5.9",
        "ext-curl": "*",
        "jms/serializer": "^1.8",
        "symfony/yaml": "^3.3",
        "symfony/validator": "^3.3",
        "php-http/client-implementation": "^1.0",
        "php-http/message": "^1.6",
        "php-http/discovery": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.3",
        "php-http/guzzle6-adapter": "^1.1"
    }
}
